# API Configuration
VITE_API_BASE_URL=http://localhost:3030
VITE_WS_URL=ws://localhost:3031

# Sync Configuration
VITE_SYNC_INTERVAL=300000  # 5 minutes in milliseconds
VITE_ENABLE_SYNC=true

# Auto Conflict Resolution
# Options: 'none' | 'server' | 'local'
# 'none' - Show dialog for each conflict
# 'server' - Always use server data (recommended)
# 'local' - Always use local data (not recommended)
VITE_AUTO_RESOLVE_CONFLICTS=server

# Development
VITE_DEBUG_SYNC=false
